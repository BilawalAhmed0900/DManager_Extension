{
  "name": "DManager",
  "version": "1.4",
  "description": "Extension for DManager!",
  "permissions": [
      "cookies",
      "downloads",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "tabs",
      "<all_urls>"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": true
  },
  "icons": { 
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png" 
  },
  "manifest_version": 2
}
