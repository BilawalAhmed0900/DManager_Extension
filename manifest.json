{
  "name": "LinkDownloader Extension",
  "version": "1.6.0",
  "description": "Extension for LinkDownloader!",
  "permissions": [
      "cookies",
      "downloads",
      "<all_urls>"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": true
  },
  "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
  },
  "manifest_version": 2
}
